# UH CAF Compiler
COMPILER:="OpenUH"
FC:="uhcaf"
NPROCS:="4"
NITER:="5"
SLEEP:="1"
FFLAGS:="-ftpp -DNPROCS=$(NPROCS) -DNITER=$(NITER)"
FFLAGS_CROSS:="-ftpp -DNPROCS=$(NPROCS) -DCROSS_ -DNITER=$(NITER)"
LAUNCHER:="cafrun -n $(NPROCS)"
TIMEOUT:="60"
BIN_PATH:="/home/sidjana/caf_testsuite/validation/bin"
COMPILE_OUTPUT:="/home/sidjana/caf_testsuite/validation/confidence_tests/compile_output"
EXEC_OUTPUT:="/home/sidjana/caf_testsuite/validation/confidence_tests/exec_output"

## Please set these variables to their appropiate values.

#Cray CAF Compiler
COMPILER="crayftn"
FC="ftn"
FFLAGS=" -F "
FFLAGS_BENCH_DEFS="-DNPROCS=${NPROCS} -DNITER=${NITER}"
LAUNCHER="aprun -n ${NPROCS} --image-heap=1G"
EXEC_OPTIONS=""
FFLAGS_CROSS=" $FFLAGS "

COMPILE_CMD="${FC} ${FFLAGS} ${FFLAGS_BENCH_DEFS}"
EXEC_CMD="${LAUNCHER} ${EXEC_OPTIONS} "

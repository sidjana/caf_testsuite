#Cray CAF Compiler
COMPILER="crayftn"
FC="crayftn"
FFLAGS="-f -DNPROCS=${NPROCS} -DNITER=${NITER} "
LAUNCHER="aprun -n ${NPROCS} --image-heap=1G"

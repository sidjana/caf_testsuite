## These variables are needed for the output files and for the naming of graphs.
## Please set these variables to their appropiate values.

#OpenUH CAF Compiler
COMPILER="OpenUH"
FC="uhcaf"
FFLAGS="-ftpp -DNPROCS=${NPROCS}"
LAUNCHER="cafrun -n ${NPROCS} --image-heap=1G"
EXEC_OPTIONS=""
FFLAGS_CROSS=" $FFLAGS "

COMPILE_CMD="${FC} ${FFLAGS} "
EXEC_CMD="${LAUNCHER} ${EXEC_OPTIONS} "
